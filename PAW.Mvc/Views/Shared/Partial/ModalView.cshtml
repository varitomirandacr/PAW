@if (TempData["ErrorMessage"] != null)
{
    <script>
        document.addEventListener('DOMContentLoaded', function () {
            Swal.fire({
                icon: '@TempData["PopupType"]' || 'error',
                title: '@TempData["ErrorMessage"]',
                showConfirmButton: false,
                timer: 5000
            });
        });
    </script>
}
